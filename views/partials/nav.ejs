  <% if (!user) { %>
    <script defer src="https://accounts.google.com/gsi/client"></script>
  <% } %>
</head>
<body>
  
  <nav id="navbar">
    <i class="fas fa-solid fa-eye fa-2x"></i>
 
    <a 
    href="/podcasts/new" <%- title === "New Podcast" ? 'class="active"' : "" %>
  >
    New Podcast
  </a>
  <a 
    href="/podcasts" <%- title === "All Podcast" ? 'class="active"' : "" %>
  >
    All Podcast
  </a>

  
    <% if (user) { %>
      <a href="/auth/logout" class="right">
        <button type="submit" id="button">Logout</button>
      </a>
    <% } else { %>
      <div 
        id="g_id_onload"
        data-client_id="<%= googleClientID %>"
        data-login_uri="/auth/google"
        data-auto_prompt="false"
      ></div>

      <div 
        class="g_id_signin right"
        data-type="standard"
        data-size="large"
        data-theme="filled_black"
        data-text="sign_in_with"
        data-shape="rectangular"
        data-logo_alignment="left"
      ></div>
    <% } %>

  </nav>
